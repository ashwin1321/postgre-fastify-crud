{
  "type": "module",
  "name": "postgrescrud",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashwin1321/postgre-fastify-crud.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashwin1321/postgre-fastify-crud/issues"
  },
  "homepage": "https://github.com/ashwin1321/postgre-fastify-crud#readme",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/postgres": "^5.2.0",
    "@fastify/sensible": "^5.0.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "0.2.0",
    "tap": "^16.1.0"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ]
  }
}
